[package]
name = "leptos_element_plus"
version = "0.0.6"
edition = "2021"
authors = ["Joey <joey.xmw@gmail.com>"]
description = "A wrapper of element-plus for leptos"
license = "MIT"
readme = "../README.md"
include = [
  "src/**/*",
  "js_dist/index.js",
  "js_dist/index.css",
]

[dependencies]
leptos = { version = "0.5.1", features = ["csr"] }
leptos_meta = { version = "0.5.1", features = ["csr"] }

wasm-bindgen = "0.2"
js-sys = "0.3.64"

serde = { version = "1.0", features = ["derive"] }
serde-wasm-bindgen = "0.5.0"
include-flate = "0.3.0"

[dev-dependencies]
wasm-bindgen = "0.2"
wasm-bindgen-test = "0.3.0"
web-sys = "0.3.64"
